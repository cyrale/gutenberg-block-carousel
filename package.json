{
  "name": "gutenberg-block-carousel",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cgb-scripts start",
    "build": "cgb-scripts build && npx uglifyjs dist/app.js --compress --mangle --output dist/app.min.js",
    "eject": "cgb-scripts eject",
    "fixjs": "npx prettier --write src/**/*.js && npx eslint --fix src/**/*.js"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "dist/app.js": [
      "npx uglifyjs --compress --mangle --output dist/app.min.js --",
      "git add"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "cgb-scripts": "1.11.0",
    "classnames": "2.2.6",
    "eslint": "5.6.1",
    "husky": "1.1.2",
    "lint-staged": "7.3.0",
    "lodash": "^4.17.21",
    "prettier": "1.14.3",
    "uglify-js": "^3.4.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  }
}
